{
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
	  "camelcase": 0,
	  "quotes": [2, "single", "avoid-escape"],
	  "strict": 0,
	  "no-path-concat": 0,
	  "no-underscore-dangle": 0,
	  "no-unused-expressions": 0,
	  "no-trailing-spaces": 0,
	  "no-shadow": 2,
	  "no-use-before-define": 0,
	  "no-extend-native": 1,
	  "no-console": 2,
	  "key-spacing": 2,
	  "no-shadow": 0,
	  "new-cap": 1,
	  "yoda": 0,
	  "consistent-return": 0,
	  "curly": [2, "multi-line"],
	  "no-alert": 2,
	  "no-caller": 0,
	  "no-mixed-requires": 0,
	  "no-process-exit": 0,
	  "no-proto": 0,
	  "object-curly-spacing": [2, "always"],
	  "eol-last": 2,
	  "eqeqeq": [2, "smart"],
	  "no-mixed-spaces-and-tabs": 2,
	  "no-unused-vars": 2,
	  "semi": [2, "always"],
	  "space-unary-ops": 2
	},
	"globals": {
		"$": true,
		"_": true,
		"it": true,
		"jQuery": true,
	  	"process": true,
	 	"NODE_ENV": true,
	  	"BootstrapDialog":true
	}
}
